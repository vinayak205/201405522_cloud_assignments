***executing 32bit.asm
nasm -f elf 32bit.asm
gcc -m32 32bit.o -o 32bit_run
./32bit_run


***executing 64bit.asm
nasm -f elf64 64bit.asm
ld 64bit.o -o 64bit_run
./64bit_run

***executing translator

g++ translator.cpp
./a.out > 64bit_convert.asm


***executing 64bit_convert.asm
nasm -f elf64 64bit_convert.asm
ld 64bit_convert.o -o 64bit_convert_run
./64bit_convert_run
